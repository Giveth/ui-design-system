{
	"name": "@giveth/ui-design-system",
	"version": "1.11.10",
	"files": [
		"/lib"
	],
	"main": "./lib/cjs/index.js",
	"module": "./lib/esm/index.js",
	"types": "./lib/esm/index.d.ts",
	"dependencies": {
		"next": "13.1.6",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"styled-components": "^5.3.6"
	},
	"scripts": {
		"build": "rm -rf lib;yarn build:esm && yarn build:cjs",
